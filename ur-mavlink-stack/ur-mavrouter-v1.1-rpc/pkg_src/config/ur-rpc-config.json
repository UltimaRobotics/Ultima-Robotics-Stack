      "ur-shared-bus/ur-mavlink-stack/ur-mavrouter/notification"
    ]
  },
  "json_added_subs": {
    "topics": [
      "direct_messaging/ur-mavrouter/requests",
      "direct_messaging/ur-mavdiscovery/responses",
      "clients/ur-mavdiscovery/heartbeat"
    ]
  }
